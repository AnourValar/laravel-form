{
    "name": "anourvalar/laravel-form",
    "description": "Laravel blade components for HTML form elements (input / select / textarea)",
    "keywords": ["laravel", "blade", "component", "form", "input", "select", "textarea", "html", "anourvalar"],
    "homepage": "https://github.com/AnourValar/laravel-form",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=8.0",
        "anourvalar/config-helper": "^3.1"
    },
    "autoload": {
        "psr-4": {"AnourValar\\LaravelForm\\": "src/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnourValar\\LaravelForm\\Providers\\LaravelFormServiceProvider"
            ]
        }
    }
}
